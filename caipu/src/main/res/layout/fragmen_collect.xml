              android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

    <com.infoholdcity.baselibrary.view.BaseToolBar
            app:commontitle="收藏"
            app:backgroundcolor="@color/white"
            app:textcolor="#000"
            app:showback="false"
            android:layout_width="match_parent"
